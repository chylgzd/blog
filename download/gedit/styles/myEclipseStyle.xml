<?xml version="1.0" ?>
<style-scheme id="myeclipsestyle" name="MyEclipseStyle" version="1.0">
	<author/>
	<_description>my eclipse style</_description>
	<style background="#f0f2f0" foreground="#2f2f2f" name="text"/>
	<style background="#red" foreground="#black" name="cursor"/>
	<style background="#447744" foreground="#aaeeaa" name="diff:added-line"/>
	<style background="#ddbb55" foreground="#lightyellow" name="diff:changed-line"/>
	<style background="#aaccaa" foreground="#336633" name="diff:removed-line"/>
	<style background="#lightgrey" foreground="#brown" name="line-numbers"/>
	<style background="#yellow" foreground="#black" name="search-match"/>
	<style background="#bg" foreground="#0000C0" name="def:function"/>
	<style background="#bg" foreground="#008000" name="def:constant" />
	<style background="#bg" foreground="#3F7F7F" name="def:specials"/>
	<style background="#bg" foreground="#darkcyan" name="def:identifier"/>
	<style background="#bg" foreground="#3F5FBF" name="def:comment"/>
	<style background="#bg" foreground="#darkgrey" name="def:preprocessor"/>
	<style background="#bg" foreground="#7F007F" name="def:type"/>
	<style background="#bg" foreground="#7F0055" name="def:statement" bold="true"/>
</style-scheme>
